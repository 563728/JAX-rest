package com.test.services;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;

import com.test.pojo.Test;

public class TestServices {
	
	static HashMap<Integer,Test> countryIdMap=new HashMap<Integer,Test>();
	
		
	public TestServices() {
		super();
		
		   Test c1=new Test(1, "India",10000);
		   Test c2=new Test(4, "China",20000);
		   Test c3=new Test(3, "Nepal",8000);
		   Test c4=new Test(2, "Bhutan",7000);
		 		 
		   countryIdMap.put(1,c1);
		   countryIdMap.put(4,c2);
		   countryIdMap.put(3,c3);
		   countryIdMap.put(2,c4);
		
	}


	public List<Test> getCountries(){
	
		List<Test> countries=new ArrayList<Test>(countryIdMap.values());
		return countries;
	}


}
