package com.test.services;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;

import com.test.pojo.Test;

public class TestServices {
	
	static HashMap<Integer,Test> testMap=new HashMap<Integer,Test>();
	
		
	public TestServices() {
		super();
		
		   Test t1=new Test(1, "Java",80);
		   Test t2=new Test(4, "JS",60);
		   Test t3=new Test(3, "Rest",50);
		   Test t4=new Test(2, "Node",50);
		 		 
		   testMap.put(1,t1);
		   testMap.put(4,t2);
		   testMap.put(3,t3);
		   testMap.put(2,t4);
		
	}


	public List<Test> getTests(){
	
		List<Test> tests=new ArrayList<Test>(testMap.values());
		return tests;
	}


}
